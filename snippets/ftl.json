{
	"#include": {
    "prefix": "include",
    "body": [
      "[#include \"$pathToFile\"]"
    ],
    "description": "Use this to include another Freemarker file"
	},
	"call a macro": {
    "prefix": "macro:call",
    "body": [
      "[@$macroName $arguments /]"
    ],
    "description": "Call a macro that you've already defined"
  },
	"#macro": {
    "prefix": "macro:define",
    "body": [
      "[#macro $macroName $arguments]\n\t$0\n[/#macro]"
    ],
    "description": "Freemarker macro tag"
  },
